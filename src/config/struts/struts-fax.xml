<struts> 
	<package name="fax" extends="struts-default">
		<action name="*_fax" class="com.sail.cot.action.fax.CotFaxAction" method="{1}">
			<result name="query">/fax/fax.jsp</result>
			<result name="modify">/fax/editor/faxEditor.jsp</result>
			<result name="jsondata">/jsondata.jsp</result>
		</action>
	</package>
</struts>  