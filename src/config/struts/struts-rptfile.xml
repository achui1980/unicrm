<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE struts PUBLIC  
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
          "http://struts.apache.org/dtds/struts-2.0.dtd">  
<struts> 
	<package name="rptfile" extends="struts-default">
		<action name="*_rptfile" class="com.sail.cot.action.system.rptfile.CotRptFileAction" method="{1}">
			<result name="query">/system/rptfile/rptFile.jsp</result>
			<result name="modify">/system/rptfile/editor/rptfileeditor.jsp</result>
			<result name="jsondata">/jsondata.jsp</result>
		</action>
	</package>
</struts>  