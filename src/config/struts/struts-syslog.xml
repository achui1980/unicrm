<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE struts PUBLIC  
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
          "http://struts.apache.org/dtds/struts-2.0.dtd">  
<struts> 
	<package name="syslog" extends="struts-default">
		<action name="*_syslog" class="com.sail.cot.action.system.syslog.CotSysLogAction" method="{1}">
			<result name="query">/system/syslog/sysLog.jsp</result>
			<result name="jsondata">/jsondata.jsp</result>
		</action>
	</package>
</struts>  